{
  "name": "sync53",
  "version": "2.1.0",
  "description": "Sync a Route53 account with a local config",
  "bin": {
    "sync53": "./bin/index.js"
  },
  "main": "./src/index.js",
  "scripts": {
    "test": "istanbul cover node_modules/mocha/bin/_mocha",
    "mocha": "mocha"
  },
  "keywords": [
    "route53",
    "aws",
    "amazon",
    "sync"
  ],
  "author": "Pat Cavit <pcavit@arena.net>",
  "license": "MIT",
  "devDependencies": {
    "istanbul": "^0.3.15",
    "mocha": "^2.2.5"
  },
  "dependencies": {
    "async": "^1.2.1",
    "aws-sdk": "^2.1.35",
    "chalk": "^1.0.0",
    "commander": "^2.8.1",
    "diff": "^1.2.0",
    "diff2": "^0.1.7",
    "joi": "^6.4.3",
    "moment": "^2.10.3",
    "object-path": "^0.9.2",
    "shelljs": "^0.5.1",
    "strip-json-comments": "^1.0.2"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/tivac/sync53.git"
  },
  "eslintConfig": {
    "env": {
      "mocha": true,
      "node": true
    },
    "rules": {
      "curly": 2,
      "dot-notation": 2,
      "eol-last": 0,
      "key-spacing": [
        2,
        {
          "beforeColon": true,
          "afterColon": true,
          "align": "colon"
        }
      ],
      "no-caller": 2,
      "no-constant-condition": 2,
      "no-dupe-args": 2,
      "no-dupe-keys": 2,
      "no-duplicate-case": 2,
      "no-empty": 2,
      "no-eval": 2,
      "no-ex-assign": 2,
      "no-extend-native": 2,
      "no-extra-parens": 2,
      "no-extra-semi": 2,
      "no-floating-decimal": 2,
      "no-func-assign": 2,
      "no-implied-eval": 2,
      "no-inner-declarations": 2,
      "no-mixed-requires": 0,
      "no-multi-spaces": [
        2,
        {
          "exceptions": {
            "VariableDeclarator": true
          }
        }
      ],
      "no-negated-in-lhs": 2,
      "no-new": 0,
      "no-obj-calls": 2,
      "no-sparse-arrays": 2,
      "no-trailing-spaces": 0,
      "no-unreachable": 2,
      "use-isnan": 2,
      "valid-typeof": 2
    }
  }
}
